{
    "manifest_version": 2,
    "name": "emp-devtools",
    "version": "0.0.1",
    "description": "Chrome 开发者工具拓展",
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "64": "icons/icon_64.png",
        "128": "icons/icon_128.png"
    },
    "devtools_page": "devtools-background.html",
    "background": {
        "scripts": [
            "build/background.js"
        ]
    },
    "permissions": [
        "http://*/*",
        "https://*/*",
        "file:///*",
        "storage"
    ]
}